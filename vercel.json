{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/send_daily_job.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/callback",
      "dest": "app.py"
    },
    {
      "src": "/api/send_daily_job",
      "dest": "api/send_daily_job.py"
    }
  ],
  "crons": [
    {
      "path": "/api/send_daily_job",
      "schedule": "0 6 * * *"
    }
  ]
}
